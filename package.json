{
  "name": "relayer",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production npm exec node ./dist/index.js",
    "test": "jest test",
    "build": "tsc",
    "lint": "npm run prettier-check && npm run eslint-check",
    "prettier-format": "npm exec -- prettier --write .",
    "prettier-check": "npm exec -- prettier --check .",
    "eslint-check": "npm exec eslint --ext=ts,tsx src",
    "dev": "nodemon --exec ts-node src/index.ts",
    "migration:create": "cd src/migrations && npx typeorm-ts-node-commonjs migration:create",
    "migrate:up": "npx typeorm-ts-node-commonjs migration:run -d src/ormconfig.ts",
    "migrate:down": "npx typeorm-ts-node-commonjs migration:revert -d src/ormconfig.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.16",
    "@types/express": "^4.17.21",
