    "dotenv": "^16.3.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@mysten/sui.js": "^0.49.1",
    "cors": "^2.8.5",
    "cron": "^3.1.6",
    "envalid": "^8.0.0",
